<div class="search">
  <%= form_tag socks_path, method: :get do %>
  <div class= "searchbar">
    <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find your sock"
  %>
    <%= submit_tag "Search", class: "btn btn-outline-primary" %>
  </div>
<% end %>
</div>


